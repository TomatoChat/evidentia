{
  "name": "evidentia",
  "version": "1.0.0",
  "description": "Brand research tool with React frontend and Flask API backend",
  "scripts": {
    "dev": "./start-dev.sh",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "pip install -r requirements.txt",
    "install:all": "npm run install:backend && npm run install:frontend",
    "start:backend": "python server.py",
    "start:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "start:prod": "cd frontend && npm start"
  },
  "keywords": [
    "brand-research",
    "competitive-analysis",
    "react",
    "flask",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@convex-dev/auth": "^0.0.87",
    "convex": "^1.25.2"
  }
}
